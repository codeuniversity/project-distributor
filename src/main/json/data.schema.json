{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "projects": {
          "$id": "/properties/data/properties/projects",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/projects/items",
            "type": "object",
            "properties": {
              "duration": {
                "$id": "/properties/data/properties/projects/items/properties/duration",
                "type": "string",
                "title": "The Duration Schema",
                "default": "",
                "examples": [
                  "FULL"
                ]
              },
              "name": {
                "$id": "/properties/data/properties/projects/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "AR Interior"
                ]
              },
              "stakeholder": {
                "$id": "/properties/data/properties/projects/items/properties/stakeholder",
                "type": "object",
                "properties": {
                  "organization": {
                    "$id": "/properties/data/properties/projects/items/properties/stakeholder/properties/organization",
                    "type": "string",
                    "title": "The Organization Schema",
                    "default": "",
                    "examples": [
                      ""
                    ]
                  }
                }
              },
              "id": {
                "$id": "/properties/data/properties/projects/items/properties/id",
                "type": "string",
                "title": "The Id Schema",
                "default": "",
                "examples": [
                  "cjk8i7k72021b0a774t8wa7wy"
                ]
              },
              "roles": {
                "$id": "/properties/data/properties/projects/items/properties/roles",
                "type": "object",
                "properties": {
                  "maxSE": {
                    "$id": "/properties/data/properties/projects/items/properties/roles/properties/maxSE",
                    "type": "integer",
                    "title": "The Maxse Schema",
                    "default": 0,
                    "examples": [
                      2
                    ]
                  },
                  "maxPM": {
                    "$id": "/properties/data/properties/projects/items/properties/roles/properties/maxPM",
                    "type": "integer",
                    "title": "The Maxpm Schema",
                    "default": 0,
                    "examples": [
                      1
                    ]
                  },
                  "maxID": {
                    "$id": "/properties/data/properties/projects/items/properties/roles/properties/maxID",
                    "type": "integer",
                    "title": "The Maxid Schema",
                    "default": 0,
                    "examples": [
                      2
                    ]
                  }
                }
              }
            }
          }
        },
        "users": {
          "$id": "/properties/data/properties/users",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/users/items",
            "type": "object",
            "properties": {
              "id": {
                "$id": "/properties/data/properties/users/items/properties/id",
                "type": "string",
                "title": "The Id Schema",
                "default": "",
                "examples": [
                  "cjk8l7tjf0fis0a77dq4uf9el"
                ]
              },
              "name": {
                "$id": "/properties/data/properties/users/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "Max Mustermann"
                ]
              },
              "applications": {
                "$id": "/properties/data/properties/users/items/properties/applications",
                "type": "array",
                "items": {
                  "$id": "/properties/data/properties/users/items/properties/applications/items",
                  "type": "object",
                  "properties": {
                    "isMastermind": {
                      "$id": "/properties/data/properties/users/items/properties/applications/items/properties/isMastermind",
                      "type": "boolean",
                      "title": "The Ismastermind Schema",
                      "default": false,
                      "examples": [
                        false
                      ]
                    },
                    "priority": {
                      "$id": "/properties/data/properties/users/items/properties/applications/items/properties/priority",
                      "type": "integer",
                      "title": "The Priority Schema",
                      "default": 0,
                      "examples": [
                        false
                      ]
                    },
                    "projectReference": {
                      "$id": "/properties/data/properties/users/items/properties/applications/items/properties/projectReference",
                      "type": "object",
                      "properties": {
                        "id": {
                          "$id": "/properties/data/properties/users/items/properties/applications/items/properties/projectReference/properties/id",
                          "type": "string",
                          "title": "The Id Schema",
                          "default": "",
                          "examples": [
                            "cjk8i7kd602dm0a77l890b4be"
                          ]
                        },
                        "name": {
                          "$id": "/properties/data/properties/users/items/properties/applications/items/propertiesReference/project/properties/name",
                          "type": "string",
                          "title": "The Name Schema",
                          "default": "",
                          "examples": [
                            "Build your own Robot"
                          ]
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
