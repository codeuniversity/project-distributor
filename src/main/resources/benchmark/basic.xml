				<unionMoveSelector>
					<changeMoveSelector/>
					<swapMoveSelector/>
					<pillarChangeMoveSelector/>
					<pillarSwapMoveSelector/>
				</unionMoveSelector>
				<acceptor>
					<entityTabuSize>5</entityTabuSize>
				</acceptor>
				<forager>
					<acceptedCountLimit>1000</acceptedCountLimit>
				</forager>
			</localSearch>
		</solver>
	</solverBenchmark>

	<solverBenchmark>
